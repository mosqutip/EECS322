((let ((args (new-tuple 1 2 3)))
     (let ((my_closure (make-closure :f args)))
       (let ((func (closure-proc my_closure)))
         (let ((the_args (closure-vars my_closure)))
	   (func the_args)))))
 (:f (a)
  (let ((arg1 (aref a 0)))
    (let ((arg2 (aref a 1)))
      (let ((arg3 (aref a 2)))
        (let ((val (+ arg1 arg2)))
	  (let ((result (+ val arg3)))
	    (print result))))))))
